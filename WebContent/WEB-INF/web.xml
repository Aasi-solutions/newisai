<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
	<web-app>
	<display-name>newisai</display-name>
	<listener>
<listener-class>org.newisai.ImayamListener</listener-class>
</listener>  
<servlet>
    <servlet-name>log4j-init</servlet-name>
    <servlet-class>org.Log4jInit</servlet-class>
    <init-param>
      <param-name>log4j-init-file</param-name>
      <param-value>WEB-INF/classes/log4j.lcf</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>

	<servlet>
		<servlet-name>ControllingServlet</servlet-name>
		<servlet-class>org.newisai.ControllingServlet</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>org.newisai.Logout</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Auto</servlet-name>
		<servlet-class>org.newisai.Auto</servlet-class>
	</servlet>
	<servlet>
    <servlet-name>Auto1</servlet-name>
    <servlet-class>org.newisai.Auto1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Auto2</servlet-name>
    <servlet-class>org.newisai.Auto2</servlet-class>
  </servlet>
	<servlet>
    <servlet-name>FileUploadServlet</servlet-name>
    <servlet-class>org.newisai.FileUploadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Activation</servlet-name>
    <servlet-class>org.newisai.Activation</servlet-class>
  </servlet>
  <servlet-mapping>
		<servlet-name>Activation</servlet-name>
		<url-pattern>/activate</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ControllingServlet</servlet-name>
		<url-pattern>/main</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Auto</servlet-name>
		<url-pattern>/auto</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
    <servlet-name>Auto1</servlet-name>
    <url-pattern>/auto1</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>Auto2</servlet-name>
    <url-pattern>/auto2</url-pattern>
  </servlet-mapping>
	<servlet-mapping>
    <servlet-name>FileUploadServlet</servlet-name>
    <url-pattern>/adminaction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
<servlet-name>Activation</servlet-name>
<url-pattern>/activate</url-pattern>
</servlet-mapping>
	<welcome-file-list>

		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
<error-page><exception-type>java.lang.Throwable</exception-type>
		<location>/error1.jsp</location></error-page>
		
	 <taglib>
    <taglib-uri>http://java.sun.com/jstl/core</taglib-uri>
    <taglib-location>/WEB-INF/tld/c.tld</taglib-location>
  </taglib>
  
 
  
  <taglib>
    <taglib-uri>http://java.sun.com/jstl/fmt</taglib-uri>
    <taglib-location>/WEB-INF/tld/fmt.tld</taglib-location>
  </taglib>
</web-app>